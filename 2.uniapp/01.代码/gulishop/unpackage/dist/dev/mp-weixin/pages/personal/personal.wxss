.header {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  background: #eed7b5;
  padding: 40rpx 0;
}
.header .userImg {
  width: 100rpx;
  height: 100rpx;
  margin: 0 50rpx 0 30rpx;
  background: #fff;
  border-radius: 50rpx;
}
.header .userInfo ._p {
  height: 50rpx;
  line-height: 50rpx;
}
.header .userInfo ._p:first-child {
  font-size: 28rpx;
}
.header .userInfo ._p:last-child {
  font-size: 24rpx;
}
.content ._h2 {
  font-size: 26rpx;
  line-height: 100rpx;
  margin-left: 5%;
}
.content .line {
  width: 90%;
  height: 2rpx;
  background: #eee;
  margin: 0 auto;
}
.content .myAssetList {
  width: 90%;
  margin: 20rpx auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.content .myAssetList .assetItem {
  width: 25%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  font-size: 24rpx;
  color: #333;
  line-height: 40rpx;
}
.content .personalList {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
  border-top: 1rpx solid #eee;
}
.content .personalList .navItem {
  width: 33.3333%;
  text-align: center;
  border-bottom: 1rpx solid #eee;
  border-right: 1rpx solid #eee;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 30rpx 0;
}
.content .personalList .navItem:nth-child(3n) {
  border-right: none;
}
.content .personalList .navItem .iconfont {
  font-size: 60rpx;
}
.content .personalList .navItem ._p {
  font-size: 24rpx;
  line-height: 40rpx;
}

